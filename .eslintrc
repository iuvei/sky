{
  "parser": "babel-eslint",
  "extends": ["enough"],
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": ["html", "vue"],
  "globals": {
    "DEBUG": false,
    "VERSION": false,
    "CONFIG": false
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": 0,
    "valid-jsdoc": 0,
    "no-useless-escape": "off"
  }
}
